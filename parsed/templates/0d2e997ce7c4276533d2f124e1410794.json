  "components/halothemes/home/home-subscription-form": "<div class=\"heading-and-description\">\n    <h3 class=\"heading\">Susbcribe To Our Newsletter</h3>\n    <p class=\"description\">Get latest insights on battery tech sent directly to you.</p>\n  </div>\n  \n  <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n      <fieldset class=\"form-fieldset\">\n          <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n          <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n          <input type=\"hidden\" name=\"check\" value=\"1\">\n          <div class=\"form-field\">\n              <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n              <div class=\"form-prefixPostfix wrap\">\n                  <input class=\"form-input self-custom-input self-col-sm-10\"\n                         id=\"nl_email\"\n                         name=\"nl_email\"\n                         type=\"email\"\n                         value=\"{{customer.email}}\"\n                         placeholder=\"Enter email address\"\n                         aria-describedby=\"alertBox-message-text\"\n                         aria-required=\"true\"\n                         autocomplete=\"email\"\n                         required\n                  >\n                  <input class=\"button button--primary form-prefixPostfix-button--postfix self-news-letter-button self-col-sm-2\" id=\"subscribe-button\" type=\"submit\" value=\"Subscribe\">\n                  <!-- {{#if theme_settings.halo_homepage_layout_4}}\n                    <button class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" arial-label=\"{{lang 'newsletter.subscribe_submit'}}\">></button>\n                  {{ else }}\n                    <input class=\"button button--primary form-prefixPostfix-button--postfix self-news-letter-button self-col-sm-2\" type=\"submit\" value=\"\">\n                  {{/if}} -->\n              </div>\n          </div>\n      </fieldset>\n  </form>\n  \n  {{#unless theme_settings.footer_style '==' 'style_2'}}\n    {{#if theme_settings.social_icon_placement_bottom '!==' 'bottom_none'}}\n        {{#if theme_settings.show_social-links}}\n            <div class=\"footer-info-col--social\" style=\"display: none;\">\n                {{> components/common/social-links}}\n              </div>\n          {{/if}}\n      {{/if}}\n  {{/unless}}\n  \n  <script>\n    var newsLetter = document.querySelector('.self-news-letter')\n    if (newsLetter) {\n      newsLetter.innerHTML = newsLetter.innerHTML.toLocaleUpperCase()\n    }\n  </script>\n  \n  <style>\n    .self-custom-input{\n      background-color: #231F20;\n      border-radius: 0%;\n    }\n  \n    .self-custom-input::placeholder{\n      color: white !important;\n    }\n  </style>  \n",
