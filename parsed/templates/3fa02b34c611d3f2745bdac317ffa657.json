{
  "components/products/add-to-cart": "    <div class=\"custom-increment-box\" data-quantity-change>\r\n        <button class=\"custom-increment-button\" data-action=\"dec\">\r\n            &minus;\r\n            <span class=\"is-srOnly\">{{lang 'products.quantity_decrease'}}</span>\r\n        </button>\r\n        <input class=\"custom-increment-input\" id=\"qty[]\" name=\"qty[]\" type=\"tel\"\r\n            value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\r\n            data-quantity-min=\"{{product.min_purchase_quantity}}\" data-quantity-max=\"{{product.max_purchase_quantity}}\"\r\n            min=\"1\" pattern=\"[0-9]*\" aria-live=\"polite\">\r\n        <button class=\"custom-increment-button\" data-action=\"inc\">\r\n            &#43;\r\n            <span class=\"is-srOnly\">{{lang 'products.quantity_increase'}}</span>\r\n        </button>\r\n    </div>\r\n    \r\n    \r\n    <div class=\"alertBox productAttributes-message\" style=\"display:none\">\r\n        <div class=\"alertBox-column alertBox-icon\">\r\n            <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\"\r\n                    height=\"24\" viewBox=\"0 0 24 24\">\r\n                    <path\r\n                        d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\">\r\n                    </path>\r\n                </svg>\r\n            </icon>\r\n        </div>\r\n        <p class=\"alertBox-column alertBox-message\"></p>\r\n    </div>\r\n    {{#or customer (unless settings.hide_price_from_guests)}}\r\n    <div class=\"form-action responsive-flex\">\r\n        <input style=\"border: none; width: 100%; height:100%;\" id=\"form-action-addToCart\" data-wait-message=\"{{lang 'products.adding_to_cart'}}\"\r\n            class=\"button button--tertiary\" type=\"submit\"\r\n            value=\"{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{#if quick_shop '==' 'quick-shop'}}{{lang 'common.form_submit'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}{{/if}}\">\r\n        <span class=\"product-status-message aria-description--hidden\">\r\n            {{lang 'products.adding_to_cart'}} {{lang 'category.add_cart_announcement'}}\r\n        </span>\r\n    </div>\r\n    {{#if this.with_wallet_buttons}}\r\n    <div class=\"add-to-cart-wallet-buttons\" data-add-to-cart-wallet-buttons>\r\n        {{> components/common/wallet-buttons}}\r\n    </div>\r\n    {{/if}}\r\n    {{/or}}\r\n<style>\r\n    .custom-increment-box {\r\n        display: inline-flex;\r\n        align-items: center;\r\n        justify-content:space-between;\r\n        background-color: rgba(33, 37, 41, 0.1);\r\n        border-radius: 6px;\r\n        padding: 6px 10px;\r\n        gap: 30px;\r\n\r\n        @media (max-width: 1200px) {\r\n            flex: 0.72;\r\n        }\r\n    }\r\n\r\n    .custom-increment-button {\r\n        background: none;\r\n        border: none;\r\n        font-size: 20px;\r\n        font-weight: 400;\r\n        color: #000;\r\n        width: 20px;\r\n        height: 20px;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n        cursor: pointer;\r\n    }\r\n\r\n    .custom-increment-input {\r\n        font-family: 'helvetic-regular';\r\n        background-color: #F2F2F2;\r\n        width: 20px;\r\n        text-align: center;\r\n        font-size: 16px;\r\n        font-weight: 700;\r\n        line-height: 32px;\r\n        background: transparent;\r\n        border: none;\r\n        outline: none;\r\n        appearance: none;\r\n        -moz-appearance: textfield;\r\n    }\r\n\r\n    .custom-increment-input::-webkit-outer-spin-button,\r\n    .custom-increment-input::-webkit-inner-spin-button {\r\n        -webkit-appearance: none;\r\n        margin: 0;\r\n    }\r\n\r\n    .responsive-flex {\r\n        width: fit-content;\r\n\r\n        @media (max-width: 1200px) {\r\n            flex: 1.35;\r\n        }\r\n    }\r\n</style>\n",
  "components/common/wallet-buttons": "{{#if wallet_buttons}}\n    {{{wallet_buttons}}}\n{{/if}}\n"
}