{
  "components/cart/totals": "<div class=\"custom-rating-cart-section for-desktop\" style=\"border-bottom:1px solid rgba(197, 197, 197, 1) ;\">\r\n\r\n    <div class=\"custom-rating-cart flex-column\">\r\n        <div class=\"stars\">\r\n            <img src=\"https://store-8xryzvxqo4.mybigcommerce.com/product_images/rating-stars.svg\" alt=\"\">\r\n        </div>\r\n        <span class=\"custom-rating\">Rated 4.88 out of 5</span>\r\n    </div>\r\n\r\n\r\n    <div class=\"customer-rating-cart flex-column\">\r\n        <img src=\"https://store-8xryzvxqo4.mybigcommerce.com/product_images/Frame%2048096879.svg\" alt=\"\" width=\"90px\"\r\n            height=\"16px\" style=\"text-align: center;\">\r\n        <span class=\"custom-rating\">24k+ happy customers</span>\r\n    </div>\r\n\r\n</div>\r\n\r\n\r\n<div class=\"cart-details\">\r\n    <h3 class=\"cart-summary-title\">Your Cart Summary</h3>\r\n</div>\r\n\r\n<ul class=\"cart-totals\">\r\n     <li class=\"\">\r\n        <div class=\"cart-total-label cart-price-p\">\r\n            <span class=\"self-cart-price\">Item Total</span>\r\n        </div>\r\n        <div class=\"cart-total-value cart-subtotal-value cart-price-p\">\r\n            {{#or customer (unless settings.hide_price_from_guests)}}\r\n            <span class=\"self-cart-price\">{{cart.sub_total.formatted}}</span>\r\n            {{else}}\r\n            {{> components/common/login-for-pricing}}\r\n            {{/or}}\r\n        </div>\r\n    </li>\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.checkout.subtotal'}}</strong>\r\n        </div>\r\n        <div class=\"cart-total-value cart-subtotal-value\">\r\n            {{#or customer (unless settings.hide_price_from_guests)}}\r\n            <span>{{cart.sub_total.formatted}}</span>\r\n            {{else}}\r\n            {{> components/common/login-for-pricing}}\r\n            {{/or}}\r\n        </div>\r\n    </li>\r\n    {{#if cart.gift_wrapping_cost.amount}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.checkout.gift_wrapping'}}:</strong>\r\n        </div>\r\n        <div class=\"cart-total-value\">\r\n            <span>{{cart.gift_wrapping_cost.formatted}}</span>\r\n        </div>\r\n    </li>\r\n    {{/if}}\r\n    {{#if cart.fees}}\r\n    {{#each cart.fees}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{display_name}}:</strong>\r\n        </div>\r\n        <div class=\"cart-total-value\">\r\n            <span>{{cost.formatted}}</span>\r\n        </div>\r\n    </li>\r\n    {{/each}}\r\n    {{/if}}\r\n    {{#if cart.shipping_handling.show_estimator}}\r\n    <!-- <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.checkout.shipping'}}:</strong>\r\n        </div>\r\n        {{> components/cart/shipping-estimator cart.shipping_handling}}\r\n    </li> -->\r\n    {{/if}}\r\n    {{#each cart.taxes}}\r\n    {{#unless included}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{name}}:</strong>\r\n        </div>\r\n        <div class=\"cart-total-value\">\r\n            <span>{{cost.formatted}}</span>\r\n        </div>\r\n    </li>\r\n    {{/unless}}\r\n    {{/each}}\r\n    {{#if cart.discount }}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.discount'}}:</strong>\r\n        </div>\r\n        <div class=\"cart-total-value\">\r\n            {{cart.discount.formatted}}\r\n        </div>\r\n    </li>\r\n    {{/if}}\r\n    <!-- {{#if cart.coupons}}\r\n    {{#each cart.coupons}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\r\n            <div>\r\n                <a href=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\r\n            </div>\r\n        </div>\r\n        <div class=\"cart-total-value\">\r\n            <span>{{discount.formatted}}</span>\r\n        </div>\r\n    </li>\r\n    {{/each}}\r\n    {{else}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.coupon_code'}}:</strong>\r\n        </div>\r\n        {{> components/cart/coupon-input}}\r\n    </li>\r\n    {{/if}} -->\r\n    {{#if settings.gift_certificates_enabled}}\r\n    {{#each cart.gift_certificates}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.gift_certificates.code_label' code=code}}</strong>\r\n            <span>\r\n                {{remaining.formatted}} | <a href=\"{{remove_url}}\">{{lang 'cart.gift_certificates.remove'}}</a>\r\n            </span>\r\n        </div>\r\n        <div class=\"cart-total-value\">\r\n            {{used.formatted}}\r\n        </div>\r\n    </li>\r\n    {{/each}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.gift_certificates.gift_certificate'}}:</strong>\r\n        </div>\r\n        {{> components/cart/gift-certificate-input}}\r\n    </li>\r\n    {{/if}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label cart-price-p2 \" style=\"font-size: 20px;\">\r\n            <span class=\"cart-price-total cart-price-p2\">Total</span>\r\n        </div>\r\n        <div class=\"cart-total-value cart-total-grandTotal cart-price-p2\">\r\n            {{#or customer (unless settings.hide_price_from_guests)}}\r\n            <span class=\"cart-price-p2 self-total-price\">{{cart.grand_total.formatted}}</span>\r\n            {{else}}\r\n            {{> components/common/login-for-pricing}}\r\n            {{/or}}\r\n        </div>\r\n        <span class=\"cart-total-label price-text cart-price-p1\" style=\"width: 75%;\">Taxes are calculated during checkout.</span>\r\n    </li>\r\n    {{#if cart.coupons}}\r\n    {{#each cart.coupons}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\r\n            <div>\r\n                <a href=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\r\n            </div>\r\n        </div>\r\n        <div class=\"cart-total-value\">\r\n            <span>{{discount.formatted}}</span>\r\n        </div>\r\n    </li>\r\n    {{/each}}\r\n    {{else}}\r\n    <li class=\"cart-total\" style=\"border: none;\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>Have a promo code?</strong>\r\n        </div>\r\n        {{> components/cart/coupon-input}}\r\n    </li>\r\n    {{/if}}\r\n    {{#each cart.taxes}}\r\n    {{#if included}}\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label\">\r\n            <strong>{{concat name (lang 'cart.included_in_total')}}:</strong>\r\n        </div>\r\n        <div class=\"cart-total-value\">\r\n            <span>{{cost.formatted}}</span>\r\n        </div>\r\n    </li>\r\n    {{/if}}\r\n    {{/each}}\r\n</ul>\r\n\r\n\r\n\r\n\r\n<style>\r\n    .custom-rating-cart-section {\r\n        max-width: 100%;\r\n        display: flex;\r\n        justify-content: space-around;\r\n        height: 60px;\r\n        margin-top: 6%;\r\n    }\r\n     .flex-column{\r\n        align-items: center !important;\r\n     }\r\n\r\n    .stars {\r\n        text-align: center;\r\n        color: #EC3B26;\r\n        font-size: 22px;\r\n\r\n    }\r\n\r\n    .custom-rating {\r\n        font-size: 16px;\r\n        font-weight: 700;\r\n        font-family: 'helvetic-regular';\r\n        color: #000000;\r\n        line-height: 136%;\r\n    }\r\n\r\n\r\n    .cart-details {\r\n        height: 40px;\r\n        width: 100%;\r\n    }\r\n\r\n    .cart-summary-title {\r\n        font-family: 'vox';\r\n        font-size: 20px;\r\n        font-weight: 400;\r\n        color: #231F20;\r\n        line-height: 25px;\r\n    }\r\n    .self-cart-price {\r\n        font-size: 16px;\r\n        font-family: 'helvetic-regular';\r\n        font-weight: 400;\r\n        line-height: 25px;\r\n        color: #231F20;\r\n\r\n    }\r\n     .cart-price-p{\r\n        padding: 1rem 0 0 0;\r\n     }\r\n     .cart-price-p1{\r\n        padding: 0 0 1rem 0;\r\n     }\r\n     .cart-price-p2{\r\n        padding: 1rem 0 0.1rem 0;\r\n     }\r\n     .cart-price-total{\r\n        font: 20px ;\r\n        font-family: 'helvetic-regular';\r\n        font-weight: 400;\r\n        line-height: 25px;\r\n     }\r\n     .price-text{\r\n        font-size: 14px;\r\n        font-family: 'helvetic-regular';\r\n        font-weight: 400;\r\n        line-height: 20px;\r\n        color: #787878;\r\n     }\r\n     .self-total-price {\r\n        font-size: 20px;\r\n        font-family: 'helvetic-regular';\r\n        font-weight: 700;\r\n        line-height: 25px;\r\n        color: #231F20;\r\n    }\r\n\r\n    @media (max-width:449px) {\r\n        .for-desktop{\r\n            display: none;\r\n        }\r\n        \r\n    }\r\n    @media (max-width:450px) {\r\n         .custom-rating {\r\n        font-size: 12px;\r\n         }\r\n         .cart-summary-title{\r\n            margin-top: 1rem;\r\n         }\r\n    }\r\n</style>\r\n",
  "components/cart/shipping-estimator": "{{#if shipping_cost}}\n    <div class=\"cart-total-value\">\n        <div class=\"subtotal shipping-estimate-show\">\n            <a href=\"#\" class=\"shipping-estimate-value\">{{shipping_cost.formatted}}</a>\n        </div>\n    </div>\n{{else}}\n    <div class=\"cart-total-value\">\n        <button data-collapsible=\"add-shipping\" class=\"shipping-estimate-show\" aria-labelledby=\"estimator-add\">\n            <span class=\"shipping-estimate-show__btn-name\">{{lang 'cart.shipping_estimator.add_info'}}</span>\n            <span id=\"estimator-add\" class=\"u-hidden\">{{lang 'cart.shipping_estimator.add_info'}}</span>\n            <span id=\"estimator-close\" class=\"u-hidden\">{{lang 'cart.shipping_estimator.cancel'}}</span>\n        </button>\n    </div>\n{{/if}}\n\n<div id=\"add-shipping\" class=\"shipping-estimator u-hidden\">\n    <form class=\"form estimator-form\" data-shipping-estimator>\n        <dl>\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_estimator.select_a_country'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\n                    {{#each countries}}\n                        <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                            {{name}}\n                        </option>\n                    {{/each}}\n                </select>\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_estimator.select_a_state'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                {{#if states.length}}\n                    <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\n                        {{#each states}}\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                                {{name}}\n                            </option>\n                        {{/each}}\n                    </select>\n                {{else}}\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\n                {{/if}}\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_estimator.suburb_city'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_estimator.suburb_city'}}\">\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_estimator.zip_postal_code'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input autocomplete=\"postal-code\" class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\">\n            </dd>\n            <button class=\"button button--tertiary button--small shipping-estimate-submit\">{{lang 'cart.shipping_estimator.estimate_shipping'}}</button>\n        </dl>\n    </form>\n    <div class=\"shipping-quotes\" role=\"alert\" aria-atomic=\"true\" aria-live=\"assertive\"></div>\n</div>\n",
  "components/cart/coupon-input": "<div class=\"cart-total-value\">\n    <button class=\"coupon-code-add d-none\" >\n        {{lang 'cart.coupons.add_coupon'}}\n\n    </button>\n    <button class=\"coupon-code-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\n</div>\n\n<div class=\"cart-form coupon-code\" >\n    <form class=\"form form--hiddenLabels coupon-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"couponcode\">{{lang 'cart.coupons.coupon_code'}}</label>\n        <input class=\"form-input placeholder-add\" data-error=\"{{lang 'cart.coupons.empty_error'}}\"  id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"Enter promocode\">\n        <input class=\"button button--tertiary button--small cart-apply-button\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n\n\n<style>\n    .cart-apply-button {\n        font-size: 16px;\n        font-weight: 700;\n        font-family: 'helvetic-regular';\n        color: #EF3826;\n        background-color: #FFFFFF;\n        border: 2px solid #EF3826;\n        padding: 14px 33px 14px 16px;\n    }\n    .placeholder-add {\n        color: #545D6B !important;\n        font-family: 'helvetic-regular';\n        font-size: 16px;\n        font-weight: 400;\n    }\n    \n</style>\n",
  "components/common/login-for-pricing": "<p translate>Log in for pricing</p>\n",
  "components/cart/gift-certificate-input": "<div class=\"cart-total-value\">\n\n    <button class=\"gift-certificate-add\">{{lang 'cart.gift_certificates.gift_certificate'}}</button>\n\n    <button class=\"gift-certificate-cancel\" style=\"display: none;\">{{lang 'cart.coupons.cancel'}}</button>\n</div>\n\n<div class=\"cart-form gift-certificate-code\" style=\"display: none;\">\n    <form class=\"form form--hiddenLabels cart-gift-certificate-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"certcode\">{{lang 'cart.gift_certificates.cert_code'}}</label>\n        <input class=\"form-input\" data-error=\"{{lang 'cart.gift_certificates.empty_error'}}\"  id=\"certcode\" type=\"text\" name=\"certcode\" value=\"\" placeholder=\"{{lang 'cart.gift_certificates.add_cert_code'}}\">\n        <input class=\"button button--tertiary button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n"
}