{
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"https://cdn11.bigcommerce.com/s-8xryzvxqo4/images/stencil/original/image-manager/frame-1707479597-1-.png?t=1750248411\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\" >\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n\n<style>\n    .header-logo-image-container{\n        height: 40px;\n        width: 53.68px;\n    }\n</style>\n"
}