{
  "components/common/breadcrumbs": "\n\n<nav aria-label=\"Breadcrumb\" class=\"self-view-on-md\">\n    <ol class=\"breadcrumbs\" style=\"display: flex; justify-content: center; flex-wrap: wrap;\">\n    {{#unless theme_settings.hide_breadcrumbs }}\n        {{#each breadcrumbs}}\n            <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\"> \n                <a class=\"breadcrumb-label\"\n                   href=\"{{url}}\"\n                   {{#if @last}}aria-current=\"page\"{{/if}}\n                >\n                    <span>{{name}}</span>\n                </a>\n            </li>\n                        {{#unless @last}}\n                <li style=\"align-content: center; font-size: 20px; font-weight: 100; color: rgba(117, 117, 117, 1); \">/</li>\n                {{/unless}}\n        {{/each}}\n    {{/unless}}\n</ol>\n\n</nav>\n\n<style>\n    .self-view-on-md{\n        display: none;\n    }\n    @media (max-width: 1199px){\n        .self-view-on-md{\n            display: block;\n        }\n\n  \n    }\n\n</style>\n\n<script type=\"application/ld+json\">\n{\n    \"@context\": \"https://schema.org\",\n    \"@type\": \"BreadcrumbList\",\n    \"itemListElement\":\n    [\n        {{#each breadcrumbs}}\n        {\n            \"@type\": \"ListItem\",\n            \"position\": {{add @index 1}},\n            \"item\": {\n                \"@id\": {{{JSONstringify url}}},\n                \"name\": {{{JSONstringify name}}}\n            }\n        }{{#unless @last}},{{/unless}}\n        {{/each}}\n    ]\n}\n</script>\n\n\n<script>\ndocument.addEventListener(\"DOMContentLoaded\", function() {\n    const breadcrumbText = document.querySelector('.breadcrumbs .breadcrumb');\n    if (!breadcrumbText) return;\n    \n    const url = window.location.href.toLowerCase();\n    const productName = document.querySelector('.productView-title')?.textContent.toLowerCase() || '';\n\n    // 1. Lead Acid condition (2.5 years)\n    if (url.includes('lead+acid') || url.includes('lead-acid') || url.includes('lead acid') || \n        productName.includes('lead+acid') || productName.includes('lead-acid') || productName.includes('lead acid')) {\n        breadcrumbText.textContent = '12 Months Free Replacement';\n    }\n    // 2. AGM condition (24 months)\n    else if (url.includes('agm') || productName.includes('agm')) {\n        breadcrumbText.textContent = '24 Months Free Replacement';\n    }\n    // 3. Lithium with 12V condition (36 months)\n    else if ((url.includes('lithium') || productName.includes('lithium')) && \n             (url.includes('12v') || productName.includes('12v'))) {\n        breadcrumbText.textContent = '36 Months Free Replacement';\n    }\n    \n    // 4. Higher voltage lithium (16v/24v/36v/48v) condition (60 months)\n    else if ((url.includes('lithium') || productName.includes('lithium')) && \n             (url.match(/16v|24v|36v|48v/) || productName.match(/16v|24v|36v|48v/))) {\n        breadcrumbText.textContent = '60 Months Free Replacement';\n    }\n    // Default remains 36 months (original behavior)\n});\n</script>\n"
}